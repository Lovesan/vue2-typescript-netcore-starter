{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "skipDefaultLibCheck": true,
        "strict": true,
        "types": [ "webpack-env", "vue", "vue-router", "bootstrap-vue" ]
    },
    "exclude": [
        "bin",
        "node_modules",
    ]
}
